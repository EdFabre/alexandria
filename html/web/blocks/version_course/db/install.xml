<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/version_course/db" VERSION="20090707" COMMENT="XMLDB file for Moodle course version tables">
  <TABLES>
    <TABLE NAME="block_version_course" COMMENT="To store version of a course">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="versionid" />
        <FIELD NAME="versionid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="versionname"/>
        <FIELD NAME="versionname" TYPE="char" LENGTH="50" NOTNULL="true" PREVIOUS="versionid" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="versionname" NEXT="changes"/>
        <FIELD NAME="changes" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="course"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="version_course_pk" TYPE="primary" FIELDS="id" COMMENT="Primary key for course versions"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="version_course_ix" UNIQUE="true" FIELDS="versionid,course" />
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>